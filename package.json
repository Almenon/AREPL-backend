{
  "name": "arepl-backend",
  "version": "1.0.0-beta.6",
  "description": "JS interface to python evaluator for AREPL",
  "main": "index.js",
  "scripts": {
    "compile": "tsc -watch -p ./",
    "compileOnce": "tsc",
    "declaration": "tsc --target es6 --declaration index.ts",
    "test": "npm run compileOnce && mocha --ui tdd *.test.js",
    "appveyorTest": "mocha --ui tdd --reporter mocha-appveyor-reporter *.test.js",
    "document": "documentation readme index.js --section=API",
    "prePublish": "npm run compileOnce && npm run test && npm run document"
  },
  "repository": "https://github.com/Almenon/arepl-backend",
  "bugs": {
    "url": "https://github.com/Almenon/arepl-backend"
  },
  "keywords": [
    "IDE",
    "python",
    "real-time",
    "AREPL"
  ],
  "files": [
    "python",
    "!__pycache__",
    "*.d.ts",
    "*.js",
    "*.js.map",
    "*.ts"
  ],
  "author": "Almenon",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^9.3.0",
    "mocha": "^3.5.3",
    "mocha-appveyor-reporter": "^0.4.0"
  },
  "dependencies": {
    "python-shell": "^0.5.0"
  }
}
